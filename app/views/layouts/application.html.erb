<!DOCTYPE html>
<html>
  <head>
    <title>Album</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="page-header">
    <% if current_user %>
      <%= link_to "Log Out", destroy_user_session_path, method: :delete, class:"users-signout-button" %>
    <% end %>
    </div>
    <%= render 'layouts/alerts' %>
    
    <%= yield %>

  </body>
</html>
